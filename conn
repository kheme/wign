###############################################

if ($DATABASE!="DATABASE") {
 if (!@mysql_connect(host,username,password)) {echo "Database connection failure!<p>Cause: ".mysql_error();exit;}
 else {mysql_select_db(database);}
}



/*----- Declatation of Functions -----*/
